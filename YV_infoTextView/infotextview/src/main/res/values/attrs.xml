<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="InfoTextView">
        <attr name="infoKey" format="reference|string" />
        <attr name="infoValue" format="reference|string" />
        <attr name="infoKeyColor" format="reference|color" />
        <attr name="infoValueColor" format="reference|color" />
        <attr name="infoBottomColor" format="reference|color" />

        <attr name="rightImgVisibility">
            <enum name="VISIBLE" value="0x00000000" />
            <enum name="INVISIBLE" value="0x00000004" />
            <enum name="GONE" value="0x00000008" />
        </attr>
        <attr name="leftImgVisibility">
            <enum name="VISIBLE" value="0x00000000" />
            <enum name="INVISIBLE" value="0x00000004" />
            <enum name="GONE" value="0x00000008" />
        </attr>
        <attr name="leftImgSrc" format="reference" />
        <attr name="rightImgSrc" format="reference" />
    </declare-styleable>

</resources>